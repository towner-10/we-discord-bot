// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Announcement {
  id    Int     @id @default(autoincrement())
  title String  @unique
  description String
  content String
  image String
  user String
  createdAt DateTime @default(now())
}
